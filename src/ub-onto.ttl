@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ubo: <http://data.ub.uio.no/onto#> .
@prefix mads: <http://www.loc.gov/mads/rdf/v1#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ubo: a owl:Ontology ;
  owl:versionInfo "0.2.0" .

ubo:Topic
  a owl:Class ;
  rdfs:subClassOf skos:Concept ,
    mads:Topic ;
  rdfs:label "Innholdsbeskrivende emne"@nb ,
    "Topic"@en ;
  rdfs:comment "A topical subject senso stricto."@en .

ubo:Place
  a owl:Class ;
  rdfs:subClassOf skos:Concept ,
    mads:Geographic ;
  rdfs:label "Sted"@nb ,
    "Place"@en ;
  rdfs:comment "A geographic place or feature."@en .

ubo:Time
  a owl:Class ;
  rdfs:subClassOf skos:Concept ,
    mads:Temporal ;
  rdfs:label "Tidsperiode"@nb ,
    "Time period"@en ;
  rdfs:comment "A temporal entity."@en .

ubo:GenreForm
  a owl:Class ;
  rdfs:subClassOf skos:Concept ,
    mads:GenreForm ;
  rdfs:label "Form/sjanger"@nb ,
    "Form/genre"@en ;
  rdfs:comment "A genre or form."@en .

ubo:VirtualComplexConcept
  a owl:Class ;
  rdfs:subClassOf skos:Concept ;  # mads:ComplexSubject ;
  rdfs:label "Virtuell emnestreng"@nb ,
    "Virtual complex concept"@en ;
  rdfs:comment "A pre-coordinated combination of two or more concepts, to be used in search refinement."@en .

ubo:ComplexConcept
  a owl:Class ;
  rdfs:subClassOf ubo:VirtualComplexConcept ;
  rdfs:label "Emnestreng"@nb ,
    "Complex concept"@en ;
  rdfs:comment "A pre-coordinated combination of two or more concepts, to be used in search refinement, and also recorded together in the same field in bibliographic records."@en .

ubo:component
  a owl:ObjectProperty ;
  rdfs:domain ubo:VirtualCompoundConcept ;
  rdfs:range skos:Concept ;
  rdfs:label "Strengkomponent"@nb ,
    "String component"@en ;
  rdfs:comment "The object is a string component of the subject."@en .

ubo:elementSymbol
  a owl:DatatypeProperty ;
  rdfs:domain skos:Concept ;
  rdfs:range rdfs:Literal ;
  rdfs:label "Kjemisk symbol"@nb ,
    "Chemical element symbol"@en .

ubo:libCode
  a owl:DatatypeProperty ;
  rdfs:domain skos:Concept ;
  rdfs:range rdfs:Literal ;
  rdfs:label "Samlingskode"@nb ,
    "Collection code"@en ;
  rdfs:comment "Collection code from the Bibsys system, identifying a collection where the concept is/was in use."@en .

ubo:MSC
  a owl:DatatypeProperty ;
  rdfs:domain skos:Concept ;
  rdfs:range rdfs:Literal ;
  rdfs:label "MSC1970/UBO"@nb ,
    "MSC1970/UBO"@en ;
  rdfs:comment "Classification code for a matching MSC class from the local version of the 1970 edition. String values since the edition has not been RDF-ized yet."@en .

ubo:ccmapperCandidates
  a owl:DatatypeProperty ;
  rdfs:domain skos:Concept ;
  rdfs:range xsd:integer ;
  rdfs:label "Antall mappingkandidater i CCMapper"@nb ,
    "Number of mapping candidates in CCMapper"@en .
